{% extends 'base.html.twig' %}


{% block title %}Mes projets
{% endblock %}

{% block body %}
	<div class="container mt-5 bg-dark text-white ">
		<div class="row">
			<div class="col">
				<table id='datatable' class="table table-striped table-bordered text-white">
					<thead>
						<tr>
							<th>Nom
							</th>
							<th>Prénom</th>
                            <th>Email</th>
                            <th>Détails</th>
						</tr>
					</thead>
					<tbody>
					{% for user in users %}						
						<tr><td>{{ user.name }}</td>
						<td>{{ user.firstname }}</td>
						<td>{{ user.email }}</td>
						<td>{{ user.telephone }}</td></tr>
						{% endfor %}
					</tbody>
				</table>
			</div>
		</div>
	</div>

{% endblock %}
